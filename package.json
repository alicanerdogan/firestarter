{
  "name": "firestarter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.1",
    "@svgr/webpack": "^5.4.0",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "css-loader": "^4.2.2",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "^6.1.0",
    "fork-ts-checker-webpack-plugin": "^5.1.0",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "mini-css-extract-plugin": "^0.11.0",
    "prettier": "^2.1.1",
    "react-refresh": "^0.8.3",
    "source-map-loader": "^1.1.0",
    "terser-webpack-plugin": "^4.1.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "core-js": "3.6.5",
    "lodash-es": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "start": "webpack-dev-server --mode=development --hot",
    "build": "webpack --mode=production",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" \"*.config.js\" -c .eslintrc.js",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\" \"*.config.js\"",
    "format:validate": "prettier --list-different \"{src,tests,webpack,.storybook,config}/**/*.{ts,tsx,js,jsx,json}\" \"*.config.js\"",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn eslint",
    "*.{json,js,jsx,ts,tsx,html}": "yarn prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn type-check"
    }
  }
}
